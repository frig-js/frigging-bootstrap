{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "semi": [2, "never"],
    "eqeqeq": [2, "allow-null"],
    "no-underscore-dangle": ["off"],
    "space-before-function-paren": ["off"],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true, "peerDependencies": true}]

  }
}
